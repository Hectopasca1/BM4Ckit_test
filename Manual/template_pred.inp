# input template

# global configs
START: 1          # 0: from scratch; 1: load checkpoint from LOAD_CHK_FILE_PATH
DEVICE: 'cuda:0'
VERBOSE: 1
EPOCH: 0
BATCH_SIZE: 64

# I/O configs
REDIRECT: false    # whether output training logs to OUTPUT_PATH or directly print on screen.
SAVE_PREDICTIONS: true  # only for predictions. Whether output predictions to a npz file.
LOAD_CHK_FILE_PATH: /path/to/your/file/best_checkpoint.pt
OUTPUT_PATH: /path/to/your/file/
OUTPUT_POSTFIX: Ef
PREDICTIONS_SAVE_FILE: /path/to/your/file/Predictions_Ef_test.npz  # path of saving predictions

# model configs
MODEL_NAME: SOME_MODEL
MODEL_CONFIG: {}
  # Model hyperparameters ...

